<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
  </head>
  <body>
    <h1> 성공</h1>
    <div>
      <table border="1" cellspacing="0">
        <thead>
          <tr>
            <th>id</th>
            <th>userid</th>
            <th>name</th>
            <th>pw</th>
          </tr>
        </thead>
        <tbody>
          <% for (let i = 0; i < data.length ;i++) {%>
          <tr>
            <td><%= data[i].id%></td>
            <td><%= data[i].userid%></td>
            <td><%= data[i].name%></td>
            <td><%= data[i].pw%></td>
            <% } %>
          </tr>
        </tbody>
      </table>
    </div>
  </body>
</html>